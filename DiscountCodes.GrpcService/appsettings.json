{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "EnableConsoleLog": true,
    "FileSizeLimitBytes": 10485760,
    "MinimumLogLevel": "Information",
    "MinimumLogLevelForConsole": "Warning",
    "PathAndFileNameBase": "logs/DiscountCode.json",
    "RetainedFileLimitCount": 50
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
